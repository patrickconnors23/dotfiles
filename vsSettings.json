{
    "window.zoomLevel": -1,
    "editor.fontFamily": "Consolas, Menlo, 'Courier New', monospace",
    "editor.fontSize": 13,
    "breadcrumbs.enabled": true,
    "files.associations": {
        "*.rmd": "markdown"
    },
    "prettier.tabWidth": 4,
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/__pycache__": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.suggestSelection": "first",
    "java.errors.incompleteClasspath.severity": "ignore",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[python]": {},
    "terminal.integrated.fontFamily": "\"Meslo LG M DZ for Powerline\"",
    "terminal.integrated.rendererType": "dom",
    "workbench.colorTheme": "Atom One Dark",
    "workbench.colorCustomizations": {
        "terminal.ansiMagenta": "#C678DD",
        "terminal.ansiYellow": "#E5C07B",
        "terminal.ansiBlue": "#61AFEF",
        "terminal.ansiBlack": "#282C34",
        "terminal.ansiGreen": "#98C379",
        "terminal.ansiRed": "#E06C75",
        "terminal.ansiBrightBlack": "#454C59",
        "terminal.ansiBrightBlue": "#69BBFF",
        "terminal.ansiBrightCyan": "#66D9E8",
        "terminal.ansiBrightGreen": "#B5E890",
        "terminal.ansiBrightMagenta": "#E48AFF",
        "terminal.ansiBrightRed": "#FF7A85",
        "terminal.ansiBrightWhite": "#CFD7E6",
        "terminal.ansiBrightYellow": "#FFD68A"
    },
    "terminal.explorerKind": "external"
}
